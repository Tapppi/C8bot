{
  "name": "c8bot",
  "version": "1.0.0",
  "description": "c8 discord botti",
  "engines": {
    "node": ">= 14.16 <15"
  },
  "scripts": {
    "dev": "nodemon",
    "start": "node build/src/index.ts",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "test": "ava",
    "test:watch": "ava --watch",
    "format": "prettier --config ./.prettierrc.json --ignore-path ./.prettierignore --write",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tapppi/C8bot.git"
  },
  "keywords": [
    "c8"
  ],
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Tapppi/C8bot/issues"
  },
  "homepage": "https://github.com/Tapppi/C8bot#readme",
  "devDependencies": {
    "@types/node": "^14.14.41",
    "ava": "^3.15.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "xo": "^0.38.2"
  },
  "dependencies": {
    "discord.js": "^12.5.3",
    "dotenv": "^8.2.0",
    "tslib": "^2.2.0"
  },
  "xo": {
    "prettier": true
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "npm run lint:fix"
    ],
    "*.json": [
      "npm run format"
    ],
    "*.geojson": [
      "npm run format"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "build/",
      "test/"
    ],
    "ext": "ts,js,jsx,json",
    "exec": "ts-node ./src/index.ts"
  }
}
